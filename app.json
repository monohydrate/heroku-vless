{
  "name": "heroku-vless",
  "description": "heroku-vless",
  "keywords": [
    "heroku-vless"
  ],
  "env": {
    "PROTOCOL": {
      "description": "vmess or vless",
      "value": "vless",
      "required": true
    },
    "UUID": {
      "description": "your ID (uuid generator)",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket path",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/monohydrate/heroku-vless",
  "repository": "https://github.com/monohydrate/heroku-vless",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
